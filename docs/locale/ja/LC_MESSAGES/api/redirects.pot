msgid "Parameters"
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPMovedPermanently:15
#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPFound:17
#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPSeeOther:22
#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPTemporaryRedirect:17
#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPPermanentRedirect:14
msgid "URI to provide as the Location header in the response."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPFound:1
msgid "302 Found."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPFound:3
msgid "The 302 (Found) status code indicates that the target resource resides temporarily under a different URI.  Since the redirection might be altered on occasion, the client ought to continue to use the effective request URI for future requests."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPFound:10
msgid "For historical reasons, a user agent MAY change the request method from POST to GET for the subsequent request.  If this behavior is undesired, the 307 (Temporary Redirect) status code can be used instead."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPFound:15
msgid "(See also: `RFC 7231, Section 6.4.3 <https://tools.ietf.org/html/rfc7231#section-6.4.3>`_)"
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPSeeOther:1
msgid "303 See Other."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPSeeOther:3
msgid "The 303 (See Other) status code indicates that the server is redirecting the user agent to a *different* resource, as indicated by a URI in the Location header field, which is intended to provide an indirect response to the original request."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPSeeOther:8
msgid "A 303 response to a GET request indicates that the origin server does not have a representation of the target resource that can be transferred over HTTP. However, the Location header in the response may be dereferenced to obtain a representation for an alternative resource. The recipient may find this alternative useful, even though it does not represent the original target resource."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPSeeOther:17
msgid "The new URI in the Location header field is not considered equivalent to the effective request URI."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPSeeOther:20
msgid "(See also: `RFC 7231, Section 6.4.4 <https://tools.ietf.org/html/rfc7231#section-6.4.4>`_)"
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPTemporaryRedirect:1
msgid "307 Temporary Redirect."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPTemporaryRedirect:3
msgid "The 307 (Temporary Redirect) status code indicates that the target resource resides temporarily under a different URI and the user agent MUST NOT change the request method if it performs an automatic redirection to that URI.  Since the redirection can change over time, the client ought to continue using the original effective request URI for future requests."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPTemporaryRedirect:12
msgid "This status code is similar to 302 (Found), except that it does not allow changing the request method from POST to GET."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPTemporaryRedirect:15
msgid "(See also: `RFC 7231, Section 6.4.7 <https://tools.ietf.org/html/rfc7231#section-6.4.7>`_)"
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPPermanentRedirect:1
msgid "308 Permanent Redirect."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPPermanentRedirect:3
msgid "The 308 (Permanent Redirect) status code indicates that the target resource has been assigned a new permanent URI."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPPermanentRedirect:8
msgid "This status code is similar to 301 (Moved Permanently), except that it does not allow changing the request method from POST to GET."
msgstr ""

#: ../../../../../anaconda3/lib/python3.7/site-packages/falcon/redirects.py:docstring of falcon.HTTPPermanentRedirect:12
msgid "(See also: `RFC 7238, Section 3 <https://tools.ietf.org/html/rfc7238#section-3>`_)"
msgstr ""

